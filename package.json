{
  "name": "debate-map",
  "version": "0.0.1",
  "description": "Project generated by generator-react-firebase",
  "engines": {
    "node": ">=4.2.0",
    "npm": "^3.0.0"
  },
  "scripts": {
    "clean": "rimraf dist",
    "compile": "better-npm-run compile",
    "lint": "eslint Source Tests Server",
    "lint:fix": "npm run lint -- --fix",
    "start": "better-npm-run start",
    "dev": "better-npm-run dev",
    "dev-with-stats": "better-npm-run dev-with-stats",
    "dev:no-debug": "npm run dev -- --no_debug",
    "test": "better-npm-run test",
    "test:dev": "npm run test -- --watch",
    "create-config": "better-npm-run create-config",
    "create-vendors": "better-npm-run create-vendors",
    "create-vendors-quick": "better-npm-run create-vendors-quick",
    "build": "better-npm-run build",
    "build:dev": "better-npm-run build:dev",
    "build:prod": "better-npm-run build:prod",
    "build:prod-quick": "better-npm-run build:prod-quick",
    "codecov": "cat coverage/*/lcov.info | codecov",
    "@just-deploy": "node ./build/deploy",
    "just-deploy:dev": "firebase deploy -P dev",
    "just-deploy:prod": "firebase deploy -P prod",
    "deploy:dev": "better-npm-run deploy:dev",
    "deploy:prod": "better-npm-run deploy:prod",
    "deploy:prod-quick": "better-npm-run deploy:prod-quick",
    "@build-stats": "node_modules\\.bin\\webpack --config build/webpack.config.js --output-file ForStats/Bundle.js --profile --json > BuildStats.json",
    "build-stats": "node_modules\\.bin\\webpack --config build/webpack.config.js --output-file ForStats/Bundle.js --profile --json > dist/ForStats/BuildStats.json"
  },
  "betterScripts": {
    "compile": {
      "command": "node bin/compile",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "build": {
      "command": "npm run clean && npm run compile",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "build:dev": {
      "command": "npm run build",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "app:*"
      }
    },
    "build:prod": {
      "command": "npm run build",
      "env": {
        "NODE_ENV": "production",
        "DEBUG": "app:*"
      }
    },
    "build:prod-quick": {
      "command": "npm run build",
      "env": {
        "NODE_ENV": "production",
        "DEBUG": "app:*",
        "QUICK": "true"
      }
    },
    "start": {
      "command": "node bin/server",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "test": {
      "command": "node ./node_modules/karma/bin/karma start build/karma.conf",
      "env": {
        "NODE_ENV": "test",
        "DEBUG": "app:*"
      }
    },
    "create-config": {
      "command": "node build/create-config",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "create-vendors": {
      "command": ".\\node_modules\\.bin\\webpack --config=config/Vendors.webpack.js",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "create-vendors-quick": {
      "command": ".\\node_modules\\.bin\\webpack --config=config/Vendors.webpack.js",
      "env": {
        "NODE_ENV": "production",
        "QUICK": "true"
      }
    },
    "dev": {
      "@command": "npm run create-config && nodemon bin/server --watch bin --watch build --watch config --watch Server",
      "command": "npm run create-config && nodemon bin/server --watch bin --watch build --watch config --watch Server --max_old_space_size=4096",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "app:*",
        "USE_TSLOADER": "true"
      }
    },
    "dev-with-stats": {
      "command": "npm run create-config && nodemon bin/server --watch bin --watch build --watch config --watch Server --max_old_space_size=4096",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "app:*",
        "USE_TSLOADER": "true",
        "OUTPUT_STATS": "true"
      }
    },
    "deploy:dev": {
      "command": "npm run create-config && npm run build && npm run just-deploy:dev",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "app:*",
        "USE_TSLOADER": "true",
        "#": "maybe temp; fix for that source-maps weren't working perfectly"
      }
    },
    "deploy:prod": {
      "command": "npm run create-config && npm run build && npm run just-deploy:prod",
      "env": {
        "NODE_ENV": "production",
        "DEBUG": "app:*"
      }
    },
    "deploy:prod-quick": {
      "command": "npm run create-config && npm run build && npm run just-deploy:prod",
      "env": {
        "NODE_ENV": "production",
        "DEBUG": "app:*",
        "QUICK": "true"
      }
    }
  },
  "repository": {
